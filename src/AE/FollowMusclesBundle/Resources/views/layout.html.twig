{# src/AE/FollowMusclesBundle/Resources/views/layout.html.twig #}

<!DOCTYPE HTML>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="follow your muscles. FM allows you to have a complete follow-up of your workout. You would have a backup of your sports sessions. In order to know which muscle you have stimulated and which ones you have forgotten. Customize your sports programs according to your own abilities and goals.">
    <meta name="author" content="ETTERS Ayoub">
    
    <title>{% block title %}AE Folllow msucles{% endblock %}</title>
    
    <!-- https://getbootstrap.com/docs/4.0/components/input-group/ -->
    
    <!-- Bootstrap core CSS -->
    <link rel="stylesheet" href="/FollowMuscles/web/css/bootstrap.min.css" type="text/css" />
    
    <!-- Custom styles for this template -->
    <link href="/FollowMuscles/web/css/shop-homepage.css" rel="stylesheet">
    
  </head>
  
  <body>

    <!-- Modal add muscles-->
    {{ include("AEFollowMusclesBundle:muscles:add.html.twig") }}
    
    <!-- Navigation navbar.html.twig-->
    {{ include("AEFollowMusclesBundle::navbar.html.twig") }}

    <!-- Page Content -->
    <div class="container">

      <div class="row">
        <!-- leftbar.html.twig-->
        {{ include("AEFollowMusclesBundle::leftbar.html.twig") }}

        <!-- /.col-lg-3 -->
        {% block body %}
        {% endblock %}

      </div>
      <!-- /.row -->

    </div>
    <!-- /.container -->

    <!-- footer.html.twig -->
    {{ include("AEFollowMusclesBundle::footer.html.twig") }}
    
    <!-- Bootstrap core JavaScript -->
    <script src="/FollowMuscles/web/js/jquery-3.2.1.slim.min.js"></script>
    <script src="/FollowMuscles/web/js/popper.min.js"></script>
    <script src="/FollowMuscles/web/js/bootstrap.min.js"></script>
    
    <!-- JS  -->
    <script src="/FollowMuscles/web/js/FollowMuscles.js"></script>
    
  </body>
</html>